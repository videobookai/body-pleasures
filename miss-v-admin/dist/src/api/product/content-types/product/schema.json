{
    "kind": "collectionType",
    "collectionName": "products",
    "info": {
        "singularName": "product",
        "pluralName": "products",
        "displayName": "Product"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "description": {
            "type": "text"
        },
        "mrp": {
            "type": "integer"
        },
        "sellingPrice": {
            "type": "integer"
        },
        "itemQuantityType": {
            "type": "string"
        },
        "slug": {
            "type": "uid",
            "targetField": "name"
        },
        "images": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "categories": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::category.category",
            "inversedBy": "products"
        }
    }
}
